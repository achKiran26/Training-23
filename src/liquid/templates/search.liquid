{% if search.performed %}
  <h1>Results</h1>
  {% for result in search.results %}
    {% if result.object_type == 'page' %}
      <a href="{{ result.url }}">
        {{ result.title }}
      </a>
    {% elsif result.object_type == 'article' %}
      <a href="{{ result.url }}">
        {{ result.title }}
      </a>
    {% else %}
      <a href="{{ result.url }}">
        {{ result.title }}
      </a>
    {% endif %}
    <p>{{ result.object_type }}</p>
  {% endfor %}
{% else %}
  <h1>Try different key</h1>
{% endif %}
